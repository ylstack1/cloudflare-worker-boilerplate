{
  "name": "@edge-manifest/cli",
  "version": "0.0.0",
  "description": "CLI for EDGE-MANIFEST",
  "type": "module",
  "bin": {
    "edge-manifest": "./dist/src/bin.js"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run"
  },
  "dependencies": {
    "@edge-manifest/core": "workspace:*",
    "@edge-manifest/generators": "workspace:*",
    "cac": "^6.7.14",
    "esbuild-register": "^3.6.0",
    "tslib": "^2.8.1",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.1.0",
    "typescript": "latest",
    "vitest": "^1.1.0"
  }
}

{
  "name": "@edge-manifest/starter",
  "version": "0.0.0",
  "description": "Cloudflare Worker template for EDGE-MANIFEST",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler d1 migrations apply edge-manifest-db --local --config wrangler.toml && wrangler dev --config wrangler.toml",
    "build": "wrangler build --config wrangler.toml",
    "deploy": "wrangler deploy --config wrangler.toml",
    "migrate:prod": "wrangler d1 migrations apply edge-manifest-db --remote --config wrangler.toml",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/openapi": "^1.4.11",
    "@sinclair/typebox": "^0.34.41",
    "drizzle-orm": "^0.36.4",
    "elysia": "1.4.18",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20251209.0",
    "@types/node": "^20.10.0",
    "typescript": "latest",
    "wrangler": "4.53.0"
  }
}
